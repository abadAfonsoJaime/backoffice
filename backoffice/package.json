{
  "name": "backoffice-server",
  "version": "1.0.0",
  "description": "Backoffice server application with MySQL database",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "seed": "node seed-admin.js",
    "seed-db": "node seed-database.js",
    "seed-users": "node seed-admin.js",
    "test": "newman run integration-tools/Backoffice-API-Tests.postman_collection.json -e integration-tools/Backoffice-API.postman_environment.json",
    "test:verbose": "newman run integration-tools/Backoffice-API-Tests.postman_collection.json -e integration-tools/Backoffice-API.postman_environment.json --verbose",
    "test:report": "newman run integration-tools/Backoffice-API-Tests.postman_collection.json -e integration-tools/Backoffice-API.postman_environment.json --reporters cli,html --reporter-html-export newman-report.html"
  },
  "keywords": [
    "backoffice",
    "mysql",
    "server"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "config": "^3.3.9",
    "cors": "^2.8.6",
    "debug": "^4.3.4",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.11.5"
  },
  "devDependencies": {
    "newman": "^6.2.1",
    "nodemon": "^3.0.2"
  }
}
